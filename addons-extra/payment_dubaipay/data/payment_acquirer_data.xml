<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
        <record id="payment_acquirer_dubaipay" model="payment.acquirer">
            <field name="name">DubaiPay</field>
            <field name="image_128" type="base64" file="payment_dubaipay/static/src/img/logo.png"/>
            <field name="provider">dubaipay</field>
            <field name="suqia_spcode">sdsd23dsd</field>
            <field name="suqia_servcode">sdhkwyie65</field>
            <field name="company_id" ref="base.main_company" />
            <field name="view_template_id" ref="dubaipay_form" />
            <field name="description" type="html">
                <p>
                    Accept payments with DubaiPay secure payment gateway
                </p>
                <ul class="list-inline">
                    <li class="list-inline-item"><i class="fa fa-check"/>Online Payment</li>
                    <li class="list-inline-item"><i class="fa fa-check"/>Payment Status Tracking</li>
                </ul>
            </field>
        </record>
    </data>

</odoo>
